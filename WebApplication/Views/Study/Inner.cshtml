@* @model TModel *@

@{
    // Layout = ;
    Layout = null;
    var name = "고양이";
    var age = 10;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <p>inner</p> 
    <p>@name 의 나이는 @age 입니다.</p>
    
    @{
        var list = new int[] {1, 2, 3, 4, 5};
    }
    @foreach (var n in list)
    {
        <p>@n 번째 foreach문 출력</p>
    }
    @for (var i = 0; i < 10; i++)
    {
        <p>@i 번째 for문</p>
    }
    @if (age == 100)
    {
        <p>일치합니다.</p>
    }
    else
    {
        <p>불일치 합니다.</p>
    }
</div>
</body>
</html>