@* @model TModel *@

@{
    Layout = null;
    ViewData["footerText"] = "하단 문구입니다.";
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <h2>@ViewBag.title</h2>
    <p>[모델 출력] 이름: @Model.Name, 나이: @Model.Age</p>
    @foreach(var member in ViewBag.memberList)
    {
        <p>[viewbag 반복 출력] 이름: @member.Name, 나이: @member.Age</p>
    }
    <p>[@ViewData["firstName"] @ViewData["lastName"]] Age: @ViewData["age"]</p>
    <h2>@ViewData["footerText"]</h2>
</div>
</body>
</html>